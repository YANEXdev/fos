
{
    "image": "mcr.microsoft.com/devcontainers/universal:2",
    "hostRequirements": {
      "cpus": 4
    },
    "customizations": {
      "codespaces": {
        "openFiles": [
          "pages/index.js"
        ]
      }
    },
    "portsAttributes": {
      "3000": {
        "label": "Client",
        "onAutoForward": "openPreview"
      },
      "5000": {
        "label": "Server"
      }
    },
    "forwardPorts": [3000, 5000]
  }